{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run build && npm run build-storybook",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "routes": [
    {
      "src": "/storybook/(.*)",
      "dest": "/storybook-static/$1"
    }
  ]
}


